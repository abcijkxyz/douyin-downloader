{
  "package": {
    "productName": "douyin-downloader",
    "version": "0.0.3"
  },
  "build": {
    "distDir": "../dist",
    "devPath": "http://localhost:8000",
    "beforeDevCommand": "yarn dev",
    "beforeBuildCommand": "yarn build"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.tauri.dev",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "resources": [],
      "externalBin": [],
      "copyright": "",
      "category": "DeveloperTool",
      "shortDescription": "",
      "longDescription": "",
      "deb": {
        "depends": [],
        "useBootstrapper": false
      },
      "macOS": {
        "frameworks": [],
        "useBootstrapper": false,
        "exceptionDomain": "",
        "signingIdentity": null,
        "providerShortName": null,
        "entitlements": null,
        "minimumSystemVersion": "10.13"
      },
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "updater": {
      "active": false
    },
    "allowlist": {
      "all": true,
      "fs": {
        "all": true,
        "scope": ["$HOME/**"]
      },
      "http": {
        "all": true,
        "request": true,
        "scope": ["https://www.iesdouyin.com/*"]
      }
    },
    "windows": [
      {
        "title": "抖音下载器",
        "width": 600,
        "height": 320,
        "visible": false,
        "resizable": false
      }
    ],
    "security": {
      "csp": null
    }
  }
}
